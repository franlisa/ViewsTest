<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xuejiao.codepathtest" >
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewActivity"></activity>
        <activity android:name=".EditTextActivity"></activity>
        <activity android:name=".ImageViewActivity"></activity>
        <activity android:name=".SpinnerActivity"></activity>
        <activity android:name=".WebViewActivity"></activity>
    </application>

</manifest>
